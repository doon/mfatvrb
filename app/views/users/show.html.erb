<p id="notice"><%= notice %></p>

<p>
<strong>Email:</strong>
<%= @user.email %>
</p>

<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
<table class="qr">
  <% @qr.modules.each_index do |x| %>
    <tr>
    <% @qr.modules.each_index do |y| %>
      <% if @qr.dark?(x,y) %>
      <td class="black"/>
    <% else %>
        <td class="white"/>
      <% end %>
      <% end %>
      </tr>
    <% end %>
    </table>
